pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        script {
          checkout scm
          def customImage = docker.build("${registry}:${env.BUILD_ID}")
        }

      }
    }

    stage('Publish') {
      steps {
        script {
          1. Real Madryt - Chelsea FC

          2. SL Benfica - Inter Mediolan

          3. Manchester City - Bayern Monachium

          4. AC Milan - SSC Napoli
        }

      }
    }

  }
  environment {
    registry = 'pawelpl/epam_cicd_online_session'
  }
}